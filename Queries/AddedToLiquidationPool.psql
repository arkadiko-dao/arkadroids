with pass1 as (
  select sender_address
  from transactions txs
  where contract_call_contract_id = 'SP2C2YFP12AJZB4MABJBAJ55XECVS7E4PMMZ89YZR.arkadiko-liquidation-pool-v1-1'
  and contract_call_function_name = 'stake'
  and status = 1
  and ( block_height > 10 /*From*/ and block_height < 10000 /*Until*/ )
)

select distinct sender_address as "Added to Liquidation Pool"
from pass1